{
  "name": "music-playlist-manager",
  "version": "1.0.0",
  "description": "A music playlist manager with Node.js, MongoDB, and Redis",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^2.0.2",
    "redis": "^4.6.7"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "controllers/**/*.js",
      "models/**/*.js",
      "middleware/**/*.js",
      "utils/**/*.js"
    ]
  },
  "keywords": [
    "music",
    "playlist",
    "nodejs",
    "mongodb",
    "redis"
  ],
  "author": "Your Name",
  "license": "MIT"
}
